@Library('my-shared-library') _
pipeline{
    
    agent any

    stages{
        stage('Git checkout'){
            steps{
              script{
                
               gitCheckout(branch:"master", url:"https://github.com/brenda-la/devopos_java_app.git")
              
              }  
            }
        }
         stage('unit test maven'){
            steps{
              script{
                
              mvnTest()
              
              }  
            }
        }
        stage('maven integration test'){
            steps{
              script{
                
              mvnIntegrationTest()
              
              }  
            }
        }
    }
}